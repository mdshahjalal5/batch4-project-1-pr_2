#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/fed/web/L2B4/2-mission-express-mongoose/7-mo-nodejs-express-foundation/batch4-project-1-pr_2/node_modules/.pnpm/typescript@5.7.2/node_modules/typescript/bin/node_modules:/mnt/fed/web/L2B4/2-mission-express-mongoose/7-mo-nodejs-express-foundation/batch4-project-1-pr_2/node_modules/.pnpm/typescript@5.7.2/node_modules/typescript/node_modules:/mnt/fed/web/L2B4/2-mission-express-mongoose/7-mo-nodejs-express-foundation/batch4-project-1-pr_2/node_modules/.pnpm/typescript@5.7.2/node_modules:/mnt/fed/web/L2B4/2-mission-express-mongoose/7-mo-nodejs-express-foundation/batch4-project-1-pr_2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/fed/web/L2B4/2-mission-express-mongoose/7-mo-nodejs-express-foundation/batch4-project-1-pr_2/node_modules/.pnpm/typescript@5.7.2/node_modules/typescript/bin/node_modules:/mnt/fed/web/L2B4/2-mission-express-mongoose/7-mo-nodejs-express-foundation/batch4-project-1-pr_2/node_modules/.pnpm/typescript@5.7.2/node_modules/typescript/node_modules:/mnt/fed/web/L2B4/2-mission-express-mongoose/7-mo-nodejs-express-foundation/batch4-project-1-pr_2/node_modules/.pnpm/typescript@5.7.2/node_modules:/mnt/fed/web/L2B4/2-mission-express-mongoose/7-mo-nodejs-express-foundation/batch4-project-1-pr_2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
